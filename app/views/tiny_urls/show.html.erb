<div class="tiny-show">
  <div class="panel panel-default">
    <div class="panel-body">
      <div class="form-group">
        <label>Given URL:</label>&nbsp;<%= @tinyUrl.url %>
      </div>
      <div class="form-group">
        <label>New URL:</label>&nbsp;<%= request.base_url %>/<%= @tinyUrl.shorten_key %>
      </div>
      <div class="button-link">
        <%= link_to "Back", root_path, :class => 'btn btn-primary' %>
      </div>
    </div>
  </div>
</div>